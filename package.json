{
  "name": "cloud-best-practice-analyzer",
  "version": "1.0.0",
  "description": "Cloud Best Practice Analyzer - Multi-tenant SaaS for AWS infrastructure analysis",
  "private": true,
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "install:all": "bun install && bun install --cwd frontend",
    "build": "bun run build --cwd frontend",
    "lint": "bun run lint --cwd frontend",
    "lint:fix": "bun run lint:fix --cwd frontend",
    "format": "bun run format --cwd frontend",
    "check": "bun run check --cwd frontend",
    "type-check": "bun run type-check --cwd frontend",
    "test": "bun test --cwd frontend",
    "dev": "bun run dev --cwd frontend",
    "sandbox": "bun run sandbox --cwd frontend",
    "deploy": "bun run deploy --cwd frontend"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "typescript": "^5.3.3"
  }
}