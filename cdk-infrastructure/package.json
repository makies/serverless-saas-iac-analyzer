{
  "name": "cloud-best-practice-analyzer-backend",
  "version": "1.0.0",
  "description": "Backend infrastructure for Cloud Best Practice Analyzer",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy",
    "deploy:all": "cdk deploy --all",
    "destroy": "cdk destroy",
    "diff": "cdk diff",
    "diff:secure": "ENABLE_CDK_NAG=true cdk diff",
    "synth:secure": "ENABLE_CDK_NAG=true cdk synth",
    "deploy:secure": "ENABLE_CDK_NAG=true cdk deploy",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "nag:enable": "export ENABLE_CDK_NAG=true",
    "nag:disable": "export ENABLE_CDK_NAG=false"
  },
  "dependencies": {
    "@aws-lambda-powertools/batch": "^2.8.0",
    "@aws-lambda-powertools/idempotency": "^2.8.0",
    "@aws-lambda-powertools/logger": "^2.22.0",
    "@aws-lambda-powertools/metrics": "^2.22.0",
    "@aws-lambda-powertools/parameters": "^2.22.0",
    "@aws-lambda-powertools/parser": "^2.8.0",
    "@aws-lambda-powertools/tracer": "^2.22.0",
    "@aws-sdk/client-bedrock": "^3.400.0",
    "@aws-sdk/client-bedrock-runtime": "^3.400.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.839.0",
    "@aws-sdk/client-dynamodb": "^3.400.0",
    "@aws-sdk/client-eventbridge": "^3.839.0",
    "@aws-sdk/client-s3": "^3.400.0",
    "@aws-sdk/client-ssm": "^3.400.0",
    "@aws-sdk/lib-dynamodb": "^3.839.0",
    "@middy/core": "^6.3.2",
    "aws-cdk-lib": "^2.204.0",
    "aws-lambda": "^1.0.7",
    "constructs": "^10.0.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.4",
    "@types/aws-lambda": "^8.10.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/uuid": "^9.0.8",
    "aws-cdk": "^2.204.0",
    "cdk-nag": "^2.36.33",
    "esbuild": "^0.25.5",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "tsx": "^4.7.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.15.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
